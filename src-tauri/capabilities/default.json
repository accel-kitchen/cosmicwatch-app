{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capabilities",
  "windows": ["main"],
  "permissions": [
    "dialog:allow-open",
    "dialog:allow-save",
    "fs:allow-write-text-file",
    {
      "identifier": "fs:scope",
      "allow": ["$DOWNLOAD/**", "**"]
    },
    "core:path:default",
    "core:app:default"
  ]
}
